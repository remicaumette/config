{
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "editor.autoIndent": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "explorer.openEditors.visible": 0,
    "docker.showExplorer": false,
    "workbench.commandPalette.history": 10,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "Menlo for Powerline",
    "terminal.integrated.shell.osx": "zsh",
    "go.autocompleteUnimportedPackages": true,
    "go.goroot": "/usr/local/opt/go/libexec",
    "workbench.colorTheme": "One Dark Pro",
    "extensions.ignoreRecommendations": false,
    "files.insertFinalNewline": true,
    "python.linting.enabled": true,
    "python.pythonPath": "/usr/local/opt/python3/bin/python3",
    "window.zoomLevel": 0,
    "rust-client.channel": "stable",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.resultsExplorer.location": "scm",
    "gitlens.gitExplorer.location": "scm",
    "gitlens.historyExplorer.location": "scm",
}
